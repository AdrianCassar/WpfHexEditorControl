<UserControl x:Class="TestDemoUI.TestGoToOffset"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestDemoUI"
             xmlns:hex="clr-namespace:WpfHexaEditor;assembly=WPFHexaEditor"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">

    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <TextBox Width="200" x:Name="txbNum"></TextBox>
            <Button Click="Button_Click" >Skip</Button>
        </StackPanel>
        <hex:DrawedHexEditor x:Name="editor"/>
    </DockPanel>
</UserControl>
